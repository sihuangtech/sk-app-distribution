server:
  backend_port: 4009
  frontend_port: 3009
  frontend_url: https://your-domain.com
  backend_url: https://your-domain.com
website:
  domain: https://your-domain.com
  title: 彩旗软件分发平台
  description: 为开发者提供便捷的应用包上传、管理和分发服务
upload:
  max_file_size: 5120
  allowed_extensions:
    - .exe
    - .msi
    - .dmg
    - .pkg
    - .deb
    - .rpm
    - .appimage
    - .apk
    - .ipa
    - .hap
    - .zip
    - .tar.gz
    - .7z
  max_files_per_app: 10000
admin:
  username: your_admin_username
  password: your_admin_password
  sessionDuration: 30
jwt:
  secret: your_jwt_secret_key_here
download:
  speed_limit_kbps: 0
geolocation:
  enabled: true
  api_provider: ipapi
  api_key: ''
  cache_duration: 86400 